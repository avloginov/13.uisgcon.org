.container
	+e("h2").title.main-title!= data.title
	if data.list
		+e("ul").list
			each item, i in data.list
				+e("li").list-item
					+e.box
						+e.box-title!= item.title
						+e.box-price!= item.price
							sup!= item.currency
						if i === 2
							- classes = "active"
						else
							- classes = ""
						+e("a").box-order.btn(class=classes href=site.orderLink target="_blank")!= item.order
