+e.info
	+e("h2").title!= title
	+e.info-content
		p
			+e("span").adress!= adress
		p
			+e("a").link(href!=mailLink)!= mailLinkText
		if times
			p
				+e("span").times
					time.map__time!= times[0]
					|  -
					time.map__time!= times[1]
if data
	+e.interactive#map-interactive(data-title!=data.title data-latitude!=data.latitude data-longitude!=data.longitude data-zoom!=data.zoom data-icon!=data.icon)
